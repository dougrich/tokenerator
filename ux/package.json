{
  "name": "ux",
  "version": "1.0.0",
  "main": "./build/start.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c ./build/tslint.json 'src/**/*.{ts,tsx}'",
    "compile": "tsc --p ./build/tsconfig.json && webpack --config ./build/webpack.config.client.js && webpack --config ./build/webpack.config.server.js && node ./build/theme.js",
    "watch:windows": "start node ./build/theme.js -w && start tsc --p ./build/tsconfig.json -w && start webpack --config ./build/webpack.config.client.js -w --debug && start webpack --config ./build/webpack.config.server.js -w",
    "watch:mac": "node ./build/theme.js -w & tsc --p ./build/tsconfig.json -w & webpack --config ./build/webpack.config.client.js -w & webpack --config ./build/webpack.config.server.js -w & node ./build/start.js -w"
  },
  "devDependencies": {
    "@dougrich/tokenerator": "0.1.0-beta.3",
    "@types/node": "^7.0.27",
    "@types/pino": "^3.0.1",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.26",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.11",
    "@types/react-router-dom": "^4.0.4",
    "@types/react-transition-group": "^1.1.0",
    "@types/seamless-immutable": "^6.1.2",
    "node-sass": "^4.5.3",
    "node-watch": "^0.5.4",
    "tslint": "^5.3.2",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "pino": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-page-transition": "^3.0.2",
    "react-router-transition": "^0.1.1",
    "react-transition-group": "^1.1.3",
    "seamless-immutable": "^7.1.2"
  }
}
